{
    "ci": {
        "collect": {
            "startServerCommand": "npm run start",
            "startServerReadyPattern": "ready",
            "url": [
                "http://localhost:3000/",
                "http://localhost:3000/login"
            ]
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "warn",
                    {
                        "minScore": 0.7
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 0.85
                    }
                ],
                "categories:best-practices": [
                    "warn",
                    {
                        "minScore": 0.8
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}